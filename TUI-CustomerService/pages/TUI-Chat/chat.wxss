.container {
  width: 100vw;
  height: 100vh;
  position: relative;
}

.tui-chatroom-navigatorbar{
  position: relative;
  /*top: 0;*/
  flex-shrink: 0;
  width: 750rpx;
  height: 480rpx;
  background-color: #006EFF;
}

.tui-chatroom-navigatorbar-back{
    margin: 50rpx 40rpx;
    width: 60rpx;
    height: 60rpx;
}
.consultant-info{
  width: 90%;
  height: 17vh;
  position: relative;
  background: #ffffff;
  border-radius: 4px;
  border-radius: 4px;
  display: flex;
  flex-direction: row;
  justify-content: left;
  margin: 0 20px;
}
.counsultant-info-left{
  margin: 48rpx;
  width: 150rpx;
}
.counsultant-avatar {
  height: 128rpx;
  width: 128rpx;
  border-radius: 64rpx;
}
.counsultant-info-middle {
  display: flex;
  flex-direction: column;
  width: 80%;
  margin: 18rpx 0;
}
.counsultant-info-text {
  font-size: 30rpx;
  margin-top: 18rpx;
}

button{
  font-size: 12pt !important;
  margin-bottom: 15px;
}
.btn {
  margin:  18px 100px;
  width: 100%;
  display: flex;
  flex-direction: column;
  position: absolute;
  line-height: 20rpx;
}


.message-list {
  flex: 1;
  width: 100vw;
  overflow-y: scroll;
}

.message-input {
  flex-shrink: 0;
  width: 100%;
}
.calling {
  position: fixed;
  z-index: 199;
  top: 0;
  bottom: 0;
  right: 0;
}
.group-profile {
  top: 176rpx;
  left: 0;
  z-index: 8
}
