.waitList-page {
  width: 100%;
  height: 100%;
}
.waitList-card {
  margin: 28rpx 28rpx;
  border: solid 1rpx #666;
  display: flex;
  flex-direction: row;
}
.waitList-left{
  margin: 48rpx 36rpx;
  width: 150rpx;
}
.counseler-avatar {
  height: 128rpx;
  width: 128rpx;
  border-radius: 64rpx;
}
.waitList-middle {
  display: flex;
  flex-direction: column;
  width: 45%;
  margin-top: 32rpx;
}
.counseler-information-text {
  font-size: 28rpx;
  margin-top: 14rpx;
}
.waitList-right{
  display: flex;
  flex-direction: column;
  margin-top: 48rpx;
  margin-bottom: 48rpx;
  border-left: solid 1rpx #666;
}
.cancel-icon {
  margin-left: 24rpx;
  width: 96rpx;
  height: 96rpx;
}
.cancel-text {
  margin-left: 24rpx;
  font-size: 24rpx;
  text-align: center;
}